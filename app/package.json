  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel backend:dev frontend:dev",
    "build": "npm-run-all --parallel frontend:build backend:build",
    "frontend:dev": "webpack-dev-server --config frontend.webpack.js",
    "frontend:build": "webpack --config frontend.webpack.js",
    "backend:dev": "npm-run-all --parallel backend:dev:webpack backend:dev:nodemon",
    "backend:dev:webpack": "webpack --watch --config backend.webpack.js",
    "backend:dev:nodemon": "nodemon --watch build/server/ build/server/main.js",
    "backend:build": "webpack --config backend.webpack.js",
    "start": "node build/server/main.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
